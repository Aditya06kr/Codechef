#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int arr[n];
	    int maxi=INT_MIN;
	    for(int i=0;i<4;i++){
	        cin>>arr[i];
	        maxi=max(maxi,arr[i]);
	    }
	    cout<<maxi<<"\n";
	}
	return 0;
}
